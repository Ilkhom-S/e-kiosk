      "displayName": "Win: MinGW x64",
      "environment": {
        "PATH": "$env{MINGW_BIN_PATH};$penv{PATH}"
      },
      "inherits": ["base", "user-paths"],
      "name": "win-mingw-x64"
    },
    {
      "cacheVariables": {
        "CMAKE_PREFIX_PATH": "$env{LINUX_QT_PATH}"
      },
      "condition": {
        "lhs": "${hostSystemName}",
        "rhs": "Linux",
        "type": "equals"
      },
      "displayName": "Linux: Qt6 x64",
      "inherits": ["base", "user-paths"],
      "name": "linux-x64"
