{
  "configurePresets": [
    {
      "binaryDir": "${sourceDir}/build/${presetName}",
      "cacheVariables": {
        "BUILD_TESTING": "ON",
        "CMAKE_EXPORT_COMPILE_COMMANDS": "YES"
      },
      "generator": "Ninja",
      "hidden": true,
      "name": "base"
    },
    {
      "environment": {
        "LINUX_QT_PATH": "/opt/Qt/6.10.1/gcc_64",
        "MINGW_BIN_PATH": "d:/Qt/Tools/mingw810_64/bin",
        "MINGW_X64_PATH": "d:/Qt/6.10.1/mingw_64",
        "QT5_X64_PATH": "d:/Qt/5.15.2/msvc2019_64",
        "QT5_X86_PATH": "d:/Qt/5.15.2/msvc2019",
        "QT6_X64_PATH": "d:/Qt/6.10.1/msvc2022_64",
        "QT6_X86_PATH": "d:/Qt/6.10.1/msvc2022"
      },
      "hidden": true,
      "name": "user-paths"
    },
    {
      "architecture": {
        "strategy": "external",
        "value": "x64"
      },
      "cacheVariables": {
        "CMAKE_PREFIX_PATH": "$env{QT6_X64_PATH}"
      },
      "displayName": "Win: Qt6 x64 Ninja",
      "inherits": ["base", "user-paths"],
      "name": "win-qt6-x64"
    },
    {
      "architecture": {
        "strategy": "external",
        "value": "x64"
      },
      "cacheVariables": {
        "CMAKE_PREFIX_PATH": "$env{QT5_X64_PATH}"
