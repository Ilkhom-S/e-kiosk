; WatchService (watchdog) конфигурационный файл - Generic/Common sections
; Platform-specific paths are merged from platform-specific templates

[Общие]
LOG_PATH=@LOG_PATH@
CHECK_TIMEOUT=60000

[taboo]
applications=explorer.exe,taskmgr.exe,processhacker.exe,iexplore.exe,browser.exe,chrome.exe,firefox.exe,opera.exe
check_timeout=60000

[Module1]
name=ekiosk
workingdirectory={WS_DIR}
startmode=auto
autostart=true
priority=1
close_priority=1
afterstartdelay=5000
maxstartcount=3
gui=true
firstpingtimeout=30000
kill_timeout=10000

[module_updater]
name=updater
workingdirectory={WS_DIR}
startmode=auto
autostart=false
priority=3
close_priority=2
afterstartdelay=0
maxstartcount=1
gui=false
firstpingtimeout=30000
kill_timeout=10000
