[taboo]
; macOS-specific taboo applications that should not be monitored/killed
applications=Activity Monitor,Terminal,Safari,Firefox,Chrome,Opera
check_timeout=60000

[Module1]
; Путь к исполняемому файлу модуля (macOS)
file={WS_DIR}/ekiosk.app/Contents/MacOS/ekiosk

[module_updater]
; Путь к исполняемому файлу модуля (macOS)
file={WS_DIR}/updater.app/Contents/MacOS/updater
