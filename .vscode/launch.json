{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug EKiosk",
      "type": "cppvsdbg",
      "request": "launch",
      "program": "${env:EK_BIN_DIR}/client.exe",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [
        {
          "name": "EK_BIN_DIR",
          "value": "${workspaceFolder}/build/win-msvc-qt5-x64/bin"
        },
        {
          "name": "PATH",
          "value": "${env:QT_BIN_PATH};${workspaceFolder}/vcpkg_installed/x64-windows/bin;${env:PATH}"
        }
      ],
      "console": "integratedTerminal",
      "preLaunchTask": "CMake: build"
    },
    {
